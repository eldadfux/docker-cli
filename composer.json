{
    "name": "appwrite/cli",
    "description": "Appwrite CLI tool",
    "type": "project",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Team Appwrite",
            "email": "team@appwrite.io"
        }
    ],
    "autoload": {
    },
    "autoload-dev": {
        "psr-4": {"Tests\\E2E\\": "tests/e2e"}
    },
    "require": {
        "php": ">=7.4.0",
        "ext-curl": "*",
        "ext-imagick": "*",
        "ext-mbstring": "*",
        "ext-json": "*",
        "ext-dom": "*",
        "ext-redis": "*",
        "ext-pdo": "*",
        "ext-openssl": "*",
        "ext-zlib": "*",
        "ext-sockets": "*",
        "utopia-php/cli": "0.5.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0"
    },
    "minimum-stability": "dev",
    "config": {
        "platform": {
            "php": "7.4"
        }
    }
}